<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Function</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.category>
          				<TCEforms>
            				<label>LLL:EXT:t3sixshop/Resources/Private/Language/locallang_db.xlf:tx_t3sixshop_flexform.singlePid</label>
            				<config>
            						<type>select</type>
            						<foreign_table>tx_t3sixshop_domain_model_category</foreign_table>
            						<foreign_table_where>AND tx_t3sixshop_domain_model_category.sys_language_uid IN (-1,0) ORDER BY tx_t3sixshop_domain_model_category.name</foreign_table_where>
            						<size>1</size>
            						<maxitems>1</maxitems>
            						<minitems>0</minitems>
            				</config>
            			</TCEforms>
            		</settings.category>
					<settings.singlePid>
          				<TCEforms>
            				<label>LLL:EXT:t3sixshop/Resources/Private/Language/locallang_db.xlf:tx_t3sixshop_flexform.singlePid</label>
            				<config>
            						<type>group</type>
            						<internal_type>db</internal_type>
            						<allowed>pages</allowed>
            						<size>1</size>
            						<maxitems>1</maxitems>
            						<minitems>0</minitems>
            				</config>
            			</TCEforms>
            		</settings.singlePid>
					<settings.list.perpage>
          				<TCEforms>
            				<label>LLL:EXT:t3sixshop/Resources/Private/Language/locallang_db.xlf:tx_t3sixshop_flexform.list.perpage</label>
            				<config>
            						<type>input</type>
            						<size>10</size>
            						<eval>int</eval>
            						<default>10</default>
            				</config>
            			</TCEforms>
            		</settings.list.perpage>
            		<settings.feature>
          				<TCEforms>
            				<label>LLL:EXT:t3sixshop/Resources/Private/Language/locallang_db.xlf:tx_t3sixshop_flexform.feature</label>
            				<config>
            						<type>check</type>
            						<default>0</default>
            				</config>
            			</TCEforms>
            		</settings.feature>
            		<settings.feature_limit>
          				<TCEforms>
            				<label>LLL:EXT:t3sixshop/Resources/Private/Language/locallang_db.xlf:tx_t3sixshop_flexform.feature.limit</label>
            				<config>
            						<type>input</type>
            						<size>10</size>
            						<eval>int</eval>
            						<default>4</default>
            				</config>
            			</TCEforms>
            		</settings.feature_limit>
				</el>
			</ROOT>
